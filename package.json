{
  "name": "@bufferapp/micro-rpc-client",
  "version": "1.5.0",
  "description": "Universal JS client for micro-rpc",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "npm run lint && jest --coverage",
    "test-watch": "jest --watch",
    "test-update": "jest -u"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bufferapp/micro-rpc-client.git"
  },
  "keywords": [
    "rpc",
    "client"
  ],
  "author": "hharnisc@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bufferapp/micro-rpc-client/issues"
  },
  "homepage": "https://github.com/bufferapp/micro-rpc-client#readme",
  "devDependencies": {
    "eslint": "^5.5.0",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^3.0.0"
  },
  "files": [
    "index.js"
  ],
  "overrides": {
    "node-fetch": "2.7.0"
  }
}
